<div id="liked" class="contatiner">
  <div class="row">
    <div class="col-lg-12">
      <table class="table table-hover">
        <tr>
          <th>Img</th>
          <th>Item</th>
          <th>Store</th>
          <th>Price</th>
          <th>Dist</th>
          <th>Clear all</th>
        </tr>  
      <% @liked_list.each do |item|%>
        <% if item.status == "like" %>
          <tr class="item">
            <td><%= image_tag(item.food_photo.photo_url) %></td>
            <td><%= item.food_photo.food_name %></td>
            <td><%= item.food_photo.store.name %></td>
            <td><%= item.food_photo.price %></td>
            <td><%= item.food_photo.store.address %></td>
            <td><%= link_to raw('<i class="fa fa-times-circle fa-1x" aria-hidden="true"></i>'), {action: 'destroy_liked_item' ,controller: 'foods', status_id: item.id}, method: :delete %></td>
            <!-- <td><i class="fa fa-times-circle fa-1x" aria-hidden="true"></i></td> -->
          </tr>
          <% end %>
      <% end %>
      </table>
    </div>
  </div>
</div>